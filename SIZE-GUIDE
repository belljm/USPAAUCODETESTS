<style>
  /* Shared styles for all size guides */
  #size-guide,
  #size-guide-babies,
  #size-guide-toddlers,
  #size-guide-boys,
  #size-guide-womens,
  #size-guide-womens-bottoms,
  #size-guide-mens-tops,
  #size-guide-mens-bottoms {
    /* empty base selector for grouping */
  }

  /* details styling */
  #size-guide details,
  #size-guide-babies details,
  #size-guide-toddlers details,
  #size-guide-boys details,
  #size-guide-womens details,
  #size-guide-womens-bottoms details,
  #size-guide-mens-tops details,
  #size-guide-mens-bottoms details {
    border: 1px solid #ccc;
    border-radius: 4px;
    margin-bottom: 1em;
    overflow: hidden;
  }

  /* summary styling */
  #size-guide summary,
  #size-guide-babies summary,
  #size-guide-toddlers summary,
  #size-guide-boys summary,
  #size-guide-womens summary,
  #size-guide-womens-bottoms summary,
  #size-guide-mens-tops summary,
  #size-guide-mens-bottoms summary {
    list-style: none;
    cursor: pointer;
    padding: 12px 20px;
    background-color: #f7f7f7;
    font-weight: 600;
    font-size: 1.1em;
    color: #222;
    user-select: none;
    position: relative;
    border-bottom: 1px solid #ddd;
    display: flex;
    justify-content: space-between;
    align-items: center;
  }

  /* hide default details marker */
  #size-guide summary::-webkit-details-marker,
  #size-guide-babies summary::-webkit-details-marker,
  #size-guide-toddlers summary::-webkit-details-marker,
  #size-guide-boys summary::-webkit-details-marker,
  #size-guide-womens summary::-webkit-details-marker,
  #size-guide-womens-bottoms summary::-webkit-details-marker,
  #size-guide-mens-tops summary::-webkit-details-marker,
  #size-guide-mens-bottoms summary::-webkit-details-marker {
    display: none;
  }

  /* custom arrow */
  #size-guide summary::after,
  #size-guide-babies summary::after,
  #size-guide-toddlers summary::after,
  #size-guide-boys summary::after,
  #size-guide-womens summary::after,
  #size-guide-womens-bottoms summary::after,
  #size-guide-mens-tops summary::after,
  #size-guide-mens-bottoms summary::after {
    content: "▸";
    font-size: 1.2em;
    transition: transform 0.3s ease;
    color: #555;
  }

  /* rotate arrow on open */
  #size-guide details[open] summary::after,
  #size-guide-babies details[open] summary::after,
  #size-guide-toddlers details[open] summary::after,
  #size-guide-boys details[open] summary::after,
  #size-guide-womens details[open] summary::after,
  #size-guide-womens-bottoms details[open] summary::after,
  #size-guide-mens-tops details[open] summary::after,
  #size-guide-mens-bottoms details[open] summary::after {
    transform: rotate(90deg);
  }

  /* details content div */
  #size-guide details > div,
  #size-guide-babies details > div,
  #size-guide-toddlers details > div,
  #size-guide-boys details > div,
  #size-guide-womens details > div,
  #size-guide-womens-bottoms details > div,
  #size-guide-mens-tops details > div,
  #size-guide-mens-bottoms details > div {
    overflow-x: auto;
    -webkit-overflow-scrolling: touch;
    background: white;
  }

  /* table styling */
  #size-guide table,
  #size-guide-babies table,
  #size-guide-toddlers table,
  #size-guide-boys table,
  #size-guide-womens table,
